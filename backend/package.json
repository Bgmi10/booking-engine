  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "nd": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "seed": "ts-node src/prisma/seed.ts",
    "flush:paymentIntent": "ts-node src/scripts/deletePaymentIntents.ts",
